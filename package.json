{
  "name": "parker-chang-web",
  "version": "1.5.6",
  "private": true,
  "scripts": {
    "og-image": "node lib/og-image-generator/app.js",
    "generate-multiple-image": "node lib/og-image-generator/generate-multiple-image.js",
    "start": "cross-env SOCKET=true node ./scripts/next-remote-watch.js ./data",
    "dev": "next dev -p  3030",
    "build": "next build && node ./scripts/generate-sitemap && node ./scripts/generate-rss",
    "generate-sitemap": "node ./scripts/generate-sitemap",
    "generate-rss": "node ./scripts/generate-rss",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install",
    "commit": "gitmoji commit"
  },
  "dependencies": {
    "@fontsource/inter": "4.5.15",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "10.4.20",
    "esbuild": "^0.23.1",
    "express": "^4.18.2",
    "front-matter": "^4.0.2",
    "github-slugger": "^1.5.0",
    "gray-matter": "^4.0.3",
    "image-size": "1.1.1",
    "marked": "^14.1.2",
    "mdast-util-to-string": "^3.2.0",
    "mdx-bundler": "^8.1.0",
    "next": "12.3.4",
    "next-themes": "^0.4.0",
    "next-translate": "^1.6.0",
    "postcss": "^8.4.24",
    "preact": "^10.15.1",
    "puppeteer": "^21.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-citation": "^0.5.0",
    "rehype-katex": "^6.0.3",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.5.1",
    "rehype-slug": "^5.1.0",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.3.2",
    "unist-util-visit": "^4.1.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.3.4",
    "@svgr/webpack": "^6.5.1",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.3.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^3.4.1",
    "file-loader": "^6.2.0",
    "globby": "11.1.0",
    "husky": "^6.0.0",
    "inquirer": "^8.2.5",
    "lint-staged": "^11.2.6",
    "next-remote-watch": "^1.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1"
  },
  "pnpm": {
    "overrides": {
      "typescript": "^3.3.1",
      "webpack": "^5.0.0",
      "autoprefixer": "10.4.20"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
